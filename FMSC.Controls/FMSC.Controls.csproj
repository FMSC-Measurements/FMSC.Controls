    <RootNamespace>FMSC.Controls</RootNamespace>
    <AssemblyName>FMSC.Controls</AssemblyName>
    <Version>1.1</Version>
    <Copyright>CC0 Public Domain</Copyright>
	<Company>USDA Forest Service</Company>
